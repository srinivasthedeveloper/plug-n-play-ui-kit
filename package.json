{
  "version": "0.0.03.10",
  "name": "plug-n-play-ui-kit",
  "description": "Plug-N-Play-Ui-Kit is a design system built with Storybook, Vite, React, TypeScript and styled-components",
  "author": "SrinivasTheDeveloper",
  "license": "MIT",
  "keywords": [
    "react",
    "typescript",
    "storybook",
    "vite",
    "styled-components",
    "plug-n-play",
    "ui-kit",
    "plug-n-play-ui-kit",
    "design-system",
    "buttons",
    "open-source"
  ],
  "homepage": "https://github.com/srinivasthedeveloper/plug-n-play-ui-kit",
  "bugs": {
    "url": "https://github.com/srinivasthedeveloper/plug-n-play-ui-kit/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/srinivasthedeveloper/plug-n-play-ui-kit.git"
  },
  "main": "dist/plug-n-play-ui-kit.umd.js",
  "module": "dist/plug-n-play-ui-kit.es.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "import": "./dist/plug-n-play-ui-kit.es.js",
      "require": "./dist/plug-n-play-ui-kit.umd.js"
    }
  },
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "rm -r dist && tsc && vite build",
    "lint": "eslint . --ext ts,tsx --fix --cache --report-unused-disable-directives --max-warnings 0",
    "format": "prettier --loglevel error --cache --write src//**/*.{ts,tsx,css} --config ./.prettierrc",
    "preview": "vite preview",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "rm:install": "rm -r node_modules/ package-lock.json && npm i"
  },
  "dependencies": {
    "babel-plugin-styled-components": "2.1.4",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "storybook": "7.5.1",
    "styled-components": "6.1.0",
    "vite-plugin-dts": "3.6.1"
  },
  "devDependencies": {
    "@storybook/addon-a11y": "7.5.1",
    "@storybook/addon-essentials": "7.5.1",
    "@storybook/addon-interactions": "7.5.1",
    "@storybook/addon-links": "7.5.1",
    "@storybook/addon-mdx-gfm": "7.5.1",
    "@storybook/addon-onboarding": "1.0.8",
    "@storybook/addon-themes": "^7.5.2",
    "@storybook/blocks": "7.5.1",
    "@storybook/react": "7.5.1",
    "@storybook/react-vite": "7.5.1",
    "@storybook/testing-library": "0.2.2",
    "@styled/storybook-dark-mode": "^1.1.5",
    "@types/react": "18.2.15",
    "@types/react-dom": "18.2.7",
    "@types/styled-components": "5.1.29",
    "@typescript-eslint/eslint-plugin": "6.9.0",
    "@typescript-eslint/parser": "6.0.0",
    "@vitejs/plugin-react-swc": "3.3.2",
    "eslint": "8.52.0",
    "eslint-config-standard-with-typescript": "39.1.1",
    "eslint-plugin-import": "2.29.0",
    "eslint-plugin-n": "16.2.0",
    "eslint-plugin-promise": "6.1.1",
    "eslint-plugin-react": "7.33.2",
    "eslint-plugin-react-hooks": "4.6.0",
    "eslint-plugin-react-refresh": "0.4.3",
    "eslint-plugin-simple-import-sort": "10.0.0",
    "eslint-plugin-storybook": "0.6.15",
    "prettier": "3.0.3",
    "react-hooks": "1.0.1",
    "typescript": "5.2.2",
    "vite": "4.4.5",
    "vite-plugin-copy": "0.1.6",
    "vite-plugin-linter": "2.0.7",
    "vite-tsconfig-paths": "4.2.1"
  }
}
